  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "e2e": "vitest run --config vitest.e2e.config.js",
    "lint:check": "npx eslint .",
    "lint:fix": "npm run lint:check -- --fix",
    "format:check": "npx prettier . --check",

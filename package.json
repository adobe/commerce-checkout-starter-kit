{
  "name": "commerce-checkout-starter-kit",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "dependencies": {
    "@adobe/aio-lib-ims": "^7.0.2",
    "@adobe/aio-lib-telemetry": "^1.1.0",
    "@adobe/aio-sdk": "^6",
    "@adobe/exc-app": "^1.4.11",
    "@adobe/react-spectrum": "^3.40.1",
    "@adobe/uix-guest": "^0.10.5",
    "cloudevents": "^4.0.2",
    "core-js": "^3.41.0",
    "dotenv": "^16.4.5",
    "got": "^11.8.6",
    "js-yaml": "^4.1.0",
    "oauth-1.0a": "^2.2.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.1.2",
    "react-router-dom": "^6.30.0",
    "regenerator-runtime": "^0.14.1",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.6",
    "@vitest/coverage-v8": "^4.0.7",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "nock": "^14.0.5",
    "ultracite": "^6.3.4",
    "vitest": "^4.0.7",
    "wait-for-expect": "^3.0.2"
  },
  "scripts": {
    "prepare": "husky",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "e2e": "vitest run --config vitest.e2e.config.js",
    "lint:check": "npx biome lint .",
    "lint": "npx biome lint --write .",
    "format:check": "npx biome format .",
    "format": "npx biome format --write .",
    "code:check": "npx biome check .",
    "code:fix": "npx biome check --write .",
    "configure-events": "node scripts/configure-events.js",
    "configure-commerce-events": "aio console workspace download ws.json && node scripts/configure-commerce-events.js ws.json; rm -f ws.json",
    "create-payment-methods": "node scripts/create-payment-methods.js payment-methods.yaml",
    "create-tax-integrations": "node scripts/create-tax-integrations.js tax-integrations.yaml",
    "create-shipping-carriers": "node scripts/create-shipping-carriers.js shipping-carriers.yaml",
    "get-shipping-carriers": "node scripts/get-shipping-carriers.js",
    "sync-oauth-credentials": "node scripts/sync-oauth-credentials.js"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "lint-staged": {
    "*": [
      "npm run code:fix"
    ]
  }
}
